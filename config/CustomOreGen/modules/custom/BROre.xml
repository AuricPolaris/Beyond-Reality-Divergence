 <!--**********   Vanilla Deposit Removal   *************-->
      <IfModInstalled name='beyondrealitycore'>

	  
	  <ConfigSection>
			<IfCondition condition=':= dimension.generator != "HellRandomLevelSource" &amp; dimension.generator != "EndRandomLevelSource"'>
				<Substitute name='OreSubstitute' block='minecraft:stone'>
					<Description> 
						Replaces all generated ore clusters with stone.   
					</Description>
					<Comment>  
						Clear the fucking ore spawn :) .
					</Comment>
						<ReplacesOre block='oreAmber' />
						<ReplacesOre block='oreAmethyst' />
						<ReplacesOre block='oreBauxite' />
						<ReplacesOre block='oreCertusQuartz' />
						<ReplacesOre block='oreCinnabar' />
						<ReplacesOre block='oreCoal' />
						<ReplacesOre block='oreCopper' />
						<ReplacesOre block='oreDarkIron' />
						<ReplacesOre block='oreDiamond' />
						<ReplacesOre block='oreEmerald' />
						<ReplacesOre block='oreGold' />
						<ReplacesOre block='oreIron' />
						<ReplacesOre block='oreLapis' />
						<ReplacesOre block='oreLead' />
						<ReplacesOre block='oreNickel' />
						<ReplacesOre block='orePoorCopper' />
						<ReplacesOre block='orePoorGold' />
						<ReplacesOre block='orePoorIron' />
						<ReplacesOre block='orePoorLead' />
						<ReplacesOre block='orePoorTin' />
						<ReplacesOre block='oreRedstone' />
						<ReplacesOre block='oreRuby' />
						<ReplacesOre block='oreSalt' />
						<ReplacesOre block='oreSaltpeter' />
						<ReplacesOre block='oreSapphire' />
						<ReplacesOre block='oreSilver' />
						<ReplacesOre block='oreSulfur' />
						<ReplacesOre block='oreTeslatite' />
						<ReplacesOre block='oreTin' />
